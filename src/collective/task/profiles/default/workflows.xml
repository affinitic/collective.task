<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">
 <property
    name="title">Contains workflow definitions for your portal</property>
 <object name="basic_task_workflow" meta_type="Workflow"/>
 <object name="task_workflow" meta_type="Workflow"/>
 <bindings>
  <type type_id="information">
   <bound-workflow workflow_id="basic_task_workflow"/>
  </type>
  <type type_id="opinion">
   <bound-workflow workflow_id="basic_task_workflow"/>
  </type>
  <type type_id="task">
    <bound-workflow workflow_id="task_workflow"/>
  </type>
  <type type_id="validation">
   <bound-workflow workflow_id="validation_workflow"/>
  </type>
 </bindings>
</object>
